version: "2"
sql:
- engine: "{{.Database.SQLCEngine }}"
  queries: "sql/queries/"
  schema: "sql/migrations/"
  gen:
    go:
      package: "repo"
      out: "internal/repo"
      emit_exact_table_names: false
      emit_empty_slices: true
      emit_prepared_queries: true
      emit_interface: true
      emit_json_tags: true
